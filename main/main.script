function init(self)
	print("This is Defold lua test start")
	local t1 = os.clock()
	local sum = 0
	for n=1,100000000,1
	do
		sum = sum + n
	end
	local t2 = os.clock()
	local msec = (t2 - t1) * 1000
	local displayText = "Sum = " .. sum .. "  Time Taken = " .. msec
	label.set_text("/go#label", displayText)
end
